import "io.goat";
import "xml.goat";

var source = load("docs/reference.xml");
if (!source)
{
    println("Can not open source XML");
    exit(-1);
}

var xml = Xml.parse(source.decode("utf8"));

var html = new Xml.Element("html");
var body = new Xml.Element("body");
html.append(body);

println(xml.findByTag("s"));
