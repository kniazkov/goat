var test = $(a, b)
{
    try
    {
        var x = a + b;
        println("ok: " + a + " + " + b + " = " + x);
    }
    catch(err)
    {
        if (err.instanceof(Exception.OperatorNotFound))
            println("fail: " + a + " + " + b);
        else
            exit();
    }
};

test(2, 3);
test('a', 'b');
println("done.");
