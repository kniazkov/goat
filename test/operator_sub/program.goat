var test = $(a, b)
{
    try
    {
        var x = a - b;
        var f = a["-"];
        var y = f.call(a, b);
        if (x == y)
            println("ok: " + a + " - " + b + " = " + x);
        else
            println("fail: " + a + " - " + b + ", x = " + x + " but y = " + y);
    }
    catch(err)
    {
        if (err.instanceof(Exception.IllegalType.OperatorNotFound))
            println("fail but ok: " + a + " - " + b);
        else
            println("exception: " + err);
    }
};

test(2, 3);
test(2, 3.1);
test(2.1, 3);
test(2.1, 3.1);

test("abc", "def");
test('a', 'b');
test(true, false);
test(null, null);
test({ }, { });
println("done.");
