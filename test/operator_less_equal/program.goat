var test = $(a, b)
{
    try
    {
        var x = a <= b;
        var f = a["<="];
        var y = f.call(a, b);
        if (x == y)
            println("ok: " + a + " <= " + b + " = " + x);
        else
            println("fail: " + a + " <= " + b + ", x = " + x + " but y = " + y);
    }
    catch(err)
    {
        if (err.instanceof(Exception.IllegalReference.OperatorNotFound))
            println("fail but ok: " + a + " <= " + b);
        else
            println("exception: " + err);
    }
};

test(2, 3);
test(3, 2);
test(2, 2);

test(2.1, 3.1);
test(3.1, 2.1);
test(2.1, 2.1);

test(2, 3.1);
test(3, 2.1);

test(2.1, 3);
test(3.1, 2);

test("abc", "abe");
test("abe", "abc");
test("abc", "ab" + "c");

test('a', 'b');
test('b', 'a');
test('a', 'a');

test(2, true);
test("abc", 13);
test(null, void);
test({}, {});

println("done.");
